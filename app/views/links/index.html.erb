<p id="notice"><%= notice %></p>

<h1>Links</h1>

<table>
  <thead>
    <tr>
      <th>Domain</th>
      <th>Source</th>
      <th>Campaign</th>
      <th>Medium</th>
      <th>Linkset</th>
      <th>Destination linkset</th>
      <th>Source linkset</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @links.each do |link| %>
      <tr>
        <td><%= link.domain %></td>
        <td><%= link.source %></td>
        <td><%= link.campaign %></td>
        <td><%= link.medium %></td>
        <td><%= link.linkset %></td>
        <td><%= link.destination_linkset %></td>
        <td><%= link.source_linkset %></td>
        <td><%= link_to 'Show', link %></td>
        <td><%= link_to 'Edit', edit_link_path(link) %></td>
        <td><%= link_to 'Destroy', link, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Link', new_link_path %>
