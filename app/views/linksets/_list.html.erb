<%- model_class = Linkset -%>
<% linksets.each do |linkset| %>
<!-- Team member -->
<div class="column is-4">
    <div class="flex-card team-card light-bordered light-raised">
        <div class="card-heading padding-10">
            <span class="tag is-success">Online</span>
        </div>
        <div class="card-body">
            <div class="avatar">
                <img src="https://place-hold.it/250x250" alt="">
            </div>
            <div class="user-id">
                <div class="name"><%= link_to linkset.name, linkset_path(linkset), class: 'main-link' %></div>
                <div class="position"><%=l linkset.created_at %></div>
                <div class="location"><i class="sl sl-icon-globe"></i>tags</div>
            </div>
            <div class="user-description">
            </div>
            <div class="card-action has-text-centered">
              <%= link_to t('.edit', :default => t("helpers.links.edit")),

              edit_linkset_path(linkset), :class => 'button btn-dash rounded accent-btn is-raised is-fullwidth no-lh' %>
            </div>
            <!-- <div class="card-action has-text-centered">
            <% if false %>
              <%= link_to t('.destroy', :default => t("helpers.links.destroy")),

              linkset_path(linkset),

              :method => :delete,

              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },

              :class => 'button btn-dash rounded accent-btn is-raised is-fullwidth ripple no-lh' %> <% end %>
            </div> -->
        </div>
    </div>
</div>
<% end %>
